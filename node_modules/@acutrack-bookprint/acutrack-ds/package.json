{
  "name": "@acutrack-bookprint/acutrack-ds",
  "version": "1.0.88",
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "description": "A scalable, reusable React-based design system for Acutrack UI consistency and fast development.",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib",
    "build:lib": "vite build --mode lib",
    "build:docs": "vite build --mode docs",
    "build:test": "vite build --mode test",
    "build:all": "npm run build:lib && npm run build:test",
    "serve:test": "vite preview --port 3000",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite .vite dist/.vite tsconfig.tsbuildinfo && npm cache clean --force",
    "clean:all": "npm run clean && rm -rf dist node_modules && npm install"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^24.7.2",
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.19",
    "path": "^0.12.7",
    "postcss": "^8.4.39",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-pwa": "^1.0.3"
  },
  "engines": {
    "node": ">=18.0.0 || >=20.0.0"
  },
  "main": "./dist/acutrack-ds.umd.js",
  "module": "./dist/acutrack-ds.es.js",
  "types": "./dist/acutrack-ds.d.ts",
  "exports": {
    ".": {
      "types": "./dist/acutrack-ds.d.ts",
      "import": "./dist/acutrack-ds.es.js",
      "require": "./dist/acutrack-ds.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./assets/*": "./dist/assets/*",
    "./docs/*": "./dist/docs/*"
  },
  "files": [
    "dist"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acutrack-bookprint/AcutrackDS.git"
  },
  "keywords": [
    "design-system",
    "react",
    "acutrack",
    "tailwind",
    "vite",
    "ui-components"
  ],
  "author": "Acutrack Team",
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "@fontsource/material-symbols-outlined": "^5.2.21",
    "@fontsource/raleway": "^5.2.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "focus-trap-react": "^11.0.6",
    "framer-motion": "^12.29.2",
    "react-day-picker": "^9.13.0",
    "react-remove-scroll": "^2.7.2",
    "react-syntax-highlighter": "^15.6.6"
  }
}
